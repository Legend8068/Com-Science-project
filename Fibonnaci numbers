num=int(input("How many numbers do you want;"))
num1, num2 = 0,1
count = 0
if num <= 0:
   print("Please enter a number above 0")
elif num == 1:
   print("Fibonacci sequence is")
   print(num1)
else:
   print("Fibonacci sequence is")
   while count < num:
       print(num1)
       n = num1 + num2
       num1 = num2
       num2 = n
       count += 1
